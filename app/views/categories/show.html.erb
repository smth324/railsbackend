<h1><%= @category.name %></h1>

<ul>
  <li><%= link_to "Destroy", category_path(@category), data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %></li>
</ul>

<h2>Types</h2>
<% @category.types.each do |type| %>
<%= type.name %>
<%end%>
