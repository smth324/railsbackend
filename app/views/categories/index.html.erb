<h1>Categories</h1>
<ul>
  <% @categories.each do |category| %>
    <li class="category">
   <%= link_to category.name, category %>
   <% category.types.each do |type| %>
   <p>
    <strong>Name:</strong>
    <%= type.name %>
  </p>
   <% end%>
    </li>
  <% end %>
</ul>
<%= link_to "New Category", new_category_path %>
